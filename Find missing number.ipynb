    " def find_missing_number(arr):\n",
    "    n = len(arr) \n",
    "    total_sum = n * (n + 1) // 2\n",
    "    actual_sum = sum(arr)\n",
    "    return total_sum - actual_sum\n",
    "\n",
    "print(find_missing_number([5, 4, 1, 0, 3]))"
   ]
